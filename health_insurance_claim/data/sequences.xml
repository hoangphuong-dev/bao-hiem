<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Claim Sequence -->
        <record id="sequence_claim" model="ir.sequence">
            <field name="name">Insurance Claim Sequence</field>
            <field name="code">hic.claim</field>
            <field name="prefix">HIC</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="clinic_auto_create">True</field>
        </record>

        <!-- Webhook Log Sequence -->
        <record id="sequence_webhook_log" model="ir.sequence">
            <field name="name">HIS Webhook Log Sequence</field>
            <field name="code">hic.webhook.log</field>
            <field name="prefix">WH</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>